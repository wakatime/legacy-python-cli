                [--log-file FILE] [--api-url URL] [--timeout SECONDS] [--sync-offline-activity AMOUNT] [--today] [--config FILE] [--config-section SECTION] [--config-read KEY] [--config-write KEY VALUE]
                [--verbose] [--version]
